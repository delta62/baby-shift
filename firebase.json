{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "*.html",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'"
          }
        ]
      }
    ]
  }
}
